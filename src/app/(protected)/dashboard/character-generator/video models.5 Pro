  23) Generate Veo 3.1 AI Video(Fast&Quality)
  const options = {
  method: 'POST',
  headers: {Authorization: 'Bearer <token>', 'Content-Type': 'application/json'},
  body: JSON.stringify({
    prompt: 'A dog playing in a park',
    imageUrls: ['http://example.com/image1.jpg', 'http://example.com/image2.jpg'],
    model: 'veo3_fast',
    watermark: 'MyBrand',
    callBackUrl: 'http://your-callback-url.com/complete',
    aspectRatio: '16:9',
    seeds: 12345,
    enableFallback: false,
    enableTranslation: true,
    generationType: 'REFERENCE_2_VIDEO'
  })
};

fetch('https://api.kie.ai/api/v1/veo/generate', options)
  .then(res => res.json())
  .then(res => console.log(res))
  .catch(err => console.error(err));